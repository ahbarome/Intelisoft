<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <sectionGroup name="spring">
      <section name="context" type="Spring.Context.Support.ContextHandler, Spring.Core"/>
      <section name="objects" type="Spring.Context.Support.DefaultSectionHandler, Spring.Core" />
    </sectionGroup>
  </configSections>

  <spring>
    <context>
      <resource uri="~/Resources/SpringContext.xml"/>
    </context>
  </spring>

  <appSettings>
    <add key="ServiceDescription" value="Servicio para la comunicación biométrica"/>
    <add key="ServiceDisplayName" value="Intelisoft BMS Service"/>
    <add key="ServiceName" value="Intelisoft BMS Service"/>
    <add key="ServiceBiometricIPDevice" value="192.168.0.125"/>
    <add key="ServiceBiometricPortDevice" value="4370"/>
    <add key="ServiceBiometricACUnlockMachineNumber" value="1"/>
    <add key="ServiceBiometricACUnlockDelay" value="30"/>

    <!-- ********* LOGGING ********* -->
    <add key="ServiceLogRequired" value="true"/>
    <add key="ServiceLogPath" value="c:\BMService.txt"/>

  </appSettings>

</configuration>